#include <stdio.h>
#include <stdlib.h>
int main()
{
    float x;
    printf("Nhap diem: ");
    scanf("%f",&x);
    if (x<5)
        printf("Yeu");
    else if (x>=5 && x<7)
        printf("TB");
    else if (x>=7 && x<8)
        printf("Kha");
    else if (x>=8 && x<9)
        printf("Gioi");
    else
        printf("Xuat Sac");
    return 0;
}
